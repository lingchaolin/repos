<%@ page language="java" contentType="text/html; charset=utf-8"
    pageEncoding="utf-8"%>
<HTML><HEAD><TITLE>中国移动业务管理系统</TITLE>
<META http-equiv=Content-Type content="text/html; charset=utf-8">
<META content="MSHTML 6.00.2600.0" name=GENERATOR>
<link href="style.css" rel="stylesheet" type="text/css">
<style type="text/css">
body,td,th {
	font-family: Verdana, Geneva, sans-serif;
}
</style>
</HEAD>
<BODY bgColor=#ffffff leftMargin=0 topMargin=0 marginheight="0" marginwidth="0">
	<br>
<table border="0" width="100%">
	<tr>
    	<td><h2>收费细项</h2></td>
    </tr>
    <tr>
    	<td>&nbsp;</td>
    </tr>
    <tr>
    	<td>
        <form name="form1" method="POST" action="">
        <table align="left" border="0">
            <tr height="40">
                <td align="right" width="30%">收费项目:</td>
                <td width="70%">
                <select size="1" name="D1">
                    <option>开户费</option>
                    <option>漫游费</option>
                    <option>押金</option>
                    <option>入网费</option>
                    <option>选号费</option>
                </select>
                </td>
            </tr>
            <tr height="40">
                <td align="right">收费金额:</td>
                <td><input type="text" name="T1" size="20" value="10"></td>
            </tr>
            <tr height="40">
                <td align="right">&nbsp;</td>
                <td><input type="submit" value="提交" name="B1"><input type="reset" value="全部重写" name="B2"></td>
            </tr>
        </table>
        </form>
        </td>
    </tr>
    <tr>
    	<td><hr></td>
    </tr>
    <tr>
    	<td>&nbsp;</td>
    </tr>
    <tr>
    	<td><h2>业务收费</h2></td>
    </tr>
    <tr>
    	<td>&nbsp;</td>
    </tr>
    <tr>
    	<td>
        <form name="form2" method="POST" action="">
        <table align="left" border="0">
            <tr height="40">
                <td align="right">业务名称:</td>
                <td colspan="5">
                <select size="1" name="D1">
                    <option>开户业务</option>
                </select>
                </td>
            </tr>
            <tr height="40">
                <td align="right">收费项目:</td>
                <td><input type="checkbox" name="C1" value="ON" checked>开户费</td>
                <td><input type="checkbox" name="C2" value="ON">漫游费</td>
                <td><input type="checkbox" name="C3" value="ON">押金</td>
                <td><input type="checkbox" name="C4" value="ON" checked>入网费</td>
                <td><input type="checkbox" name="C5" value="ON" checked>选号费</td>
            </tr>
            <tr height="40">
                <td align="right">&nbsp;</td>
                <td colspan="5"><input type="submit" value="提交" name="B1"><input type="reset" value="全部重写" name="B2"></td>
            </tr>
        </table>
        </form> 
        </td>
    </tr>
</table>
</BODY></HTML>
