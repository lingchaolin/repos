package com.aowin.javabasic;

import java.io.File;
import java.io.IOException;
import java.net.MalformedURLException;
import java.net.URL;

import org.apache.commons.io.FileUtils;

public class CopyPic {
	public static void getPic(String url,String name) throws Exception {
		FileUtils.copyURLToFile(new URL(url), new File(name));
	}
	public static void main(String[] args) {
		String url="https://timgsa.baidu.com/timg?image&quality=80&size=b9999_10000&sec=1543323753617&di=7940025d27df0ea3ef05b6c10e54f373&imgtype=0&src=http%3A%2F%2Fserver.zzidc.com%2Fuploads%2Fallimg%2F141022%2F1-141022133550227.jpg";
		String name="new.jpg";
		
	}
}
