package com.aowin;

public class Client0201 {
/**
 * 中介者模式--同事对象；
 * 
 * @param args
 */
	public static void main(String[] args) {
		Mediator m=new President();
		
		MarKet market =new MarKet(m);
		Finacial fina =new Finacial(m);
		Development devp =new Development(m);
		
		market.selfAction();
		
	}

}
