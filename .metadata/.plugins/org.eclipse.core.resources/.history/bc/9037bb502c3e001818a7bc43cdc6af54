package HomeWork;

public class HomeWork203 {	
	public static void main(String[]args){
		long total=0;
		long count=1;
		for(int i=1;i<=10;i++){
			count=1;
			for(int j=1;j<=i;j++){
				count*=j;
				
//				System.out.println("count="+count+",total="+total);
			}
			total+=count;
			System.out.println(i+"£¡="+count+",total="+total);
		}
	}
}
