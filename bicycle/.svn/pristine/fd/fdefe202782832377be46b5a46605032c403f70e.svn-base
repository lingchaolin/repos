<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="dao.CardRecordMapper">



    <insert id="addCardRecord" parameterType="model.Card_record">
        <selectKey keyProperty="recordId" resultType="int" order="BEFORE">
            select SEQ_LOG.nextval recordId from dual
        </selectKey>
        insert into card_record values (#{recordId},#{cardId},#{feeType},#{chgMonthlyMoney},#{chgWalletMoney},#{chgFrozenMoney},#{createTime}
        ,#{userId},#{remark,jdbcType=VARCHAR},#{zxbj,jdbcType=NUMERIC})
    </insert>

    
</mapper>