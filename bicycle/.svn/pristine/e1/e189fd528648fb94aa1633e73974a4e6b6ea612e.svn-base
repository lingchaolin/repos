<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="dao.RepairRecordMapper">
  	<insert id="addRepairRecord" parameterType="model.RepairRecord">
		<selectKey order="BEFORE" resultType="int" keyProperty="recordId">
			select nextId.nextval recordId from dual
		</selectKey>
		insert into 
			REPAIR_RECORD 
		values 
			(#{recordId},#{bicycleId},#{repairType},#{repairPart},#{repairResult},#{fee},#{repairer},#{repairDate},#{remark},#{userId},#{operatorTime})
	</insert>
  </mapper>