<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:schemaLocation="http://www.springframework.org/schema/beans 
							http://www.springframework.org/schema/beans/spring-beans.xsd">
		<!--构造器注入  -->
	<bean id="date" class="java.util.Date"/>
	<bean id="sequenceThree" class="com.aowin.IOC.SequenceThree">
		<constructor-arg index="0" value="1002"></constructor-arg>
		<constructor-arg index="1" value="Jack"></constructor-arg>
		<constructor-arg index="2" ref="date"></constructor-arg>
	</bean>
</beans>