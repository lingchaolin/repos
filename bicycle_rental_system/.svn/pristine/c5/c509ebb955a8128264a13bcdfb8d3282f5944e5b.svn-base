<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html >
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>新增卡</title>
</head>
<body>
	<from action="" method="">
	卡号:<input type="text" /><br/>
	卡类型:<select>
	<option value="1">社保卡</option>
	<option value="2">D普通租车卡</option>
	<option value="3">A临时租车卡</option>
	<option value="4">员工卡</option>
	<option value="5">调度卡</option>
	</select><br/>
	姓名:<input type="text" /><br/>
	身份证号:<input type="text" /><br/>
	性别:<input type="radio" value="男" name="sex"/>男
	<input type="radio" value="女" name="sex" />女<br/>
	联系电话:<input type="text" /><br/>
	手机:<input type="text" /><br/>
	邮箱:<input type="text" /><br/>
	住址:<input type="text" /><br/>
	工作单位:<input type="text" /><br/>
	注销标志:<select>
	<option value="0">正常</option>
	<option value="1">已注销</option>
	</select><br/>
	月票金额:<input type="text" /><br/>
	冻结金额:<input type="text" /><br/>
	钱包余额:<input type="text" /><br/>
	卡状态:<select>
	<option value="0">正常</option>
	
	</select><br/>
	办卡时间:<input type="date"/><br/>
	经办人:<input type="text" readonly value="登录人名"><br/>
	<input type="submit" value="注册"/> <input type="reset" value="取消"/>
	</from>
</body>
</html>