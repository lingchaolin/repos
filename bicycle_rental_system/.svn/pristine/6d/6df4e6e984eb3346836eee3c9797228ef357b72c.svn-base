<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>车辆购入</title>
<style type="text/css">
#send{
text-align:center;
}
</style>
</head>
<body>
	<h4 align="center">购入车辆表</h4>
	<table width="80%" border="1" cellspacing="0" align="center">
		<tr>
			<td>单据编号:<input type="text"/></td>
			<td>供应商ID:<input type="text"/></td>
			<td>名称:<input type="text"/></td>
			<td>地址:<input type="text"/></td>
		</tr>
		<tr>
			<td>联系电话:<input type="text"/></td>
			<td>联系人:<input type="text"/></td>
			<td>生产许可证:<input type="text"/></td>
			<td>工商注册号:<input type="text"/></td>
		</tr>
		<tr>
			<td>注册资金:<input type="text"/></td>
			<td>操作日:<input type="text"/></td>
			<td>操作日期:<input type="date"/></td>
			<td>注销标志::<input type="text"/></td>
		</tr>
		<tr>
			<td colspan="4">备注:<input type="text"/></td>
		</tr>
	
	</table>
	<hr/>
	<h4 align="center">购入车辆明细表</h4>
	<table width="80%" border="1" cellspacing="0" align="center">
		<tr>
			<th><input type="checkbox"/>全选</th>
			<th>明细单ID</th>
			<th>类型编号</th>
			<th>单据ID</th>
			<th>车辆ID</th>
			<th>出厂日期</th>
			<th>批次号</th>
			<th>价格</th>
			<th>备注</th>
		</tr>
		<tr>
			<td><input type="checkbox"/></td>
			<td>100</td>
			<td>111</td>
			<td>222</td>
			<td>333</td>
			<td>2018-9-1</td>
			<td>11112</td>
			<td>222</td>
			<td>无信息</td>
			
		</tr>
		<tr>
			<td colspan="9"><input type="button" value="确认"></td>
		</tr>
	</table>
	<br/><br/>
	<div id="send"><input type="submit" value="新增" />&nbsp;&nbsp;<input type="reset" value="取消"/></div>
</body>
</html>