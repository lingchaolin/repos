<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html >
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
function show1(){
	document.getElementById("weihuUL").style.display=(document.getElementById("weihuUL").style.display=="none"?"block":"none");
}
function show2(){
	document.getElementById("sheji").style.display=(document.getElementById("sheji").style.display=="none"?"block":"none");
}
function show3(){
	document.getElementById("card").style.display=(document.getElementById("card").style.display=="none"?"block":"none");
}
function show4(){
	document.getElementById("total").style.display=(document.getElementById("total").style.display=="none"?"block":"none");
}
function showStage3(){
	document.getElementById("stage3").style.display=(document.getElementById("stage3").style.display=="none"?"block":"none");
}
function showInformation(){
	document.getElementById("information").style.display=(document.getElementById("information").style.display=="none"?"block":"none");
}
function showCar(){
	document.getElementById("car").style.display=(document.getElementById("car").style.display=="none"?"block":"none");
}
</script>
</head>
<body>

[1]<span  onclick="show1()" style="color:red">基础信息维护</span><ul id="weihuUL" style="list-style-type:decimal;display:none">
	<li  ><span onclick="showStage3()" style="color:blue ">组织机构信息</span>
		<ul id="stage3" style="display:none">
			<li>权限</li>
			<li>角色</li>
			<li>人员登记分配</li>
		</ul>
	</li>
	<li ><span onclick="showInformation()" style="color:blue ">业务基础信息</span>
		<ul id="information" style="display:none">
		<li>供应商维护</li>
		<li>办卡</li>
		</ul>
	</li>
	<li ><span onclick="showCar()" style="color:blue ">车辆信息</span>
		<ul id="car" style="display:none">
		<li>车点车桩信息</li>
		<li>车辆类型信息</li>
		</ul>
	</li>
</ul><br/>
[2]<span onclick="show2()" style="color:red">车辆业务设计</span><ul id="sheji"   style="list-style-type:decimal;display:none">
	<li>车辆购入</li>
	<li>新车入桩</li>
	<li>车辆租出</li>
	<li>车辆归还</li>
	<li>车辆维修调出</li>
	<li>车辆维修</li>
	<li>车辆维修调入</li>
	<li>车辆调出</li>
	<li>车辆调入</li>
	<li>车辆报废</li>
</ul><br/>
[3]<span onclick="show3()" style="color:red">卡业务设计</span><ul id="card" style="list-style-type:decimal;display:none">
	<li>卡充值</li>
</ul><br/>
[4]<span  onclick="show4()" style="color:red">查询统计设计</span><ul id="total"   style="list-style-type:decimal;display:none">
	<li>车点车辆信息联合查询</li>
	<li>月度/年度车辆费用统计</li>
	<li>车点使用率信息统计分析</li>
	<li>实时、卡费用统计</li>
	<li>月度/年度卡费用统计</li>
</ul><br/>



</body>
</html>