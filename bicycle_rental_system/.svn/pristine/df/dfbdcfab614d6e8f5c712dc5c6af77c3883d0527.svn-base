package com.aowin.bicycle.maintenance.pojo;

import java.io.Serializable;


//х╗оч
public class Authority implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private Integer authid;
	private String name;
	private String authdescribe;
	
	public Authority() {
		super();
	}

	public Authority(Integer authid, String name, String authdescribe) {
		super();
		this.authid = authid;
		this.name = name;
		this.authdescribe = authdescribe;
	}

	public Integer getAuthid() {
		return authid;
	}

	public void setAuthid(Integer authid) {
		this.authid = authid;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getAuthdescribe() {
		return authdescribe;
	}

	public void setAuthdescribe(String authdescribe) {
		this.authdescribe = authdescribe;
	}

	@Override
	public String toString() {
		return "Authority [authid=" + authid + ", name=" + name + ", authdescribe=" + authdescribe + "]";
	}
	
	
	


	

	
	

}
